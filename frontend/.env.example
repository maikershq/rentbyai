# RentBy Frontend - Environment Variables Template
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.rentby.ai
# For development: http://localhost:3000

# Solana Configuration
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.devnet.solana.com
# For mainnet: https://api.mainnet-beta.solana.com

NEXT_PUBLIC_SOLANA_NETWORK=devnet
# For mainnet: mainnet-beta

# Program ID (update after deploying smart contract)
NEXT_PUBLIC_PROGRAM_ID=your_deployed_program_id_here

# Optional: Analytics
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_POSTHOG_KEY=phc_XXXXXXXXXXXX
